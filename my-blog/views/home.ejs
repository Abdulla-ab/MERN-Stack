<%- include("partials/header") %>

<body>
    <h1>Home</h1>
    <p><%= startingContent %></p>

    <% posts.forEach(function(post) { %>
        <h2><%= post.title %></h2>
        <div>
            <span><p><%= post.body.substring(0, 100).concat('...'); %></p></span>
            <span><a href="/posts/<%= post.title %>">Read More</a></span>
        </div>
    <% }); %>
</body>

<%- include("partials/footer") %>
</html>